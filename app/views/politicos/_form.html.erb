<%= form_for @politico, :html => { :class => 'form-horizontal' } do |f| %>
  <fieldset>
    <legend><%= controller.action_name.capitalize %> /Politico</legend>

    <div class="control-group">
      <%= f.label :email, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :email, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :encrypted_password, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :encrypted_password, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :reset_password_token, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :reset_password_token, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :reset_password_sent_at, :class => 'control-label' %>
      <div class="controls">
        <%= f.datetime_select :reset_password_sent_at, :class => 'datetime_select' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :remember_created_at, :class => 'control-label' %>
      <div class="controls">
        <%= f.datetime_select :remember_created_at, :class => 'datetime_select' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :sign_in_count, :class => 'control-label' %>
      <div class="controls">
        <%= f.number_field :sign_in_count, :class => 'number_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :current_sign_in_at, :class => 'control-label' %>
      <div class="controls">
        <%= f.datetime_select :current_sign_in_at, :class => 'datetime_select' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :last_sign_in_at, :class => 'control-label' %>
      <div class="controls">
        <%= f.datetime_select :last_sign_in_at, :class => 'datetime_select' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :current_sign_in_ip, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :current_sign_in_ip, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :last_sign_in_ip, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :last_sign_in_ip, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :partido_id, :class => 'control-label' %>
      <div class="controls">
        <%= f.number_field :partido_id, :class => 'number_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :id_parlamentar, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :id_parlamentar, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :nome, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :nome, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :nome_parlamentar, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :nome_parlamentar, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :sexo, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :sexo, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :uf, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :uf, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :gabinete, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :gabinete, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :anexo, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :anexo, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :fone, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :fone, :class => 'text_field' %>
      </div>
    </div>

    <div class="control-group">
      <%= f.label :foto, :class => 'control-label' %>
      <div class="controls">
        <%= f.text_field :foto, :class => 'text_field' %>
      </div>
    </div>

    <div class="form-actions">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to 'Cancel', politico_path, :class => 'btn' %>
    </div>
  </fieldset>
<% end %>
